type Nft = record {
  canister: principal;
  index: nat64;
  metadata: text;
};

type Geolocation = record { 
  latitude: float64; 
  longitude: float64; 
};

type AreaResponse = record {
  lat_start: float64;
  lon_start: float64;
  lat_end: float64;
  lon_end: float64;
  geohash: text;
  nft_square: vec Nft; 
};

service : {
  compute_geohash: (Geolocation) -> (AreaResponse);
  compute_area: (text) -> (AreaResponse);
}